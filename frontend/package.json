{
  "name": "tunescore-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite dev --port 5128",
    "preview": "vite preview --host 0.0.0.0 --port 5128",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "schema:pull": "curl -s https://music.quilty.app/api/v1/openapi.json -o openapi.json",
    "schema:gen": "openapi-zod-client --input openapi.json --output src/lib/api/types.gen.ts",
    "typecheck": "tsc -p . --noEmit",
    "types": "npm run schema:pull && npm run schema:gen"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^5.4.0",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.49.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.4",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "eslint": "^9.39.0",
    "eslint-config-prettier": "^10.1.0",
    "eslint-plugin-svelte": "^3.13.0",
    "openapi-zod-client": "^1.18.3",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte": "^5.43.0",
    "svelte-check": "^4.3.4",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.3",
    "vite": "^5.4.21"
  },
  "dependencies": {
    "@fontsource/fraunces": "^5.2.9",
    "@fontsource/inter": "^5.2.8",
    "@tailwindcss/typography": "^0.5.18",
    "@types/d3": "^7.4.3",
    "autoprefixer": "^10.4.22",
    "bits-ui": "^2.14.4",
    "chart.js": "^4.5.1",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "date-fns": "^4.1.0",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-svelte": "^8.6.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.4",
    "lucide-svelte": "^0.554.0",
    "postcss": "^8.5.6",
    "schema-dts": "^1.1.5",
    "svelte-i18n": "^4.0.1",
    "svelte-meta-tags": "^4.5.0",
    "svelte-motion": "^0.12.2",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^3.4.18",
    "zod": "^3.25.76"
  }
}
